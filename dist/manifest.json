{
   "manifest_version": 3,
   "name": "Logins For Zhen",
   "version": "2.0.0",
   "description": "",
   "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
   "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "js": ["scripts/content_script.js"],
        "css": [],
        "matches": ["https://*/*","http://*/*","file:///*"]
      }
    ],
    "permissions": ["storage", "unlimitedStorage"]
}